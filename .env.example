# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_BASE_URL=ws://localhost:8000

# Optional API key for backend communication
VITE_API_KEY=development-key

# Feature flags
VITE_ENABLE_BATCH_PROCESSING=true
VITE_MAX_CONCURRENT_JOBS=5

# Security Configuration
# CRITICAL: Generate a secure encryption key for production
# Example: openssl rand -hex 32
VITE_ENCRYPTION_KEY=your_secure_encryption_key_here

# Testing Configuration
VITE_TEST_USER_PASSWORD=your_test_user_password
VITE_TEST_ADMIN_PASSWORD=your_test_admin_password

# Development settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug