{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts", 
    "src/**/*", 
    "*.ts", 
    "*.tsx"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".astro"
  ],
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "isolatedModules": true,
    "verbatimModuleSyntax": true
  }
}
